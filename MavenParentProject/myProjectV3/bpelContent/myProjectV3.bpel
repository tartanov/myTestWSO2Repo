<bpel:process name="myProjectV3"
         targetNamespace="http://wso2.org/bps/sample"
         suppressJoinFailure="yes"
         xmlns:tns="http://wso2.org/bps/sample"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns="http://www.example.org/HelloWorldService/">

    <!-- Import the client WSDL -->
	<bpel:import location="myProjectV3Artifacts.wsdl" namespace="http://wso2.org/bps/sample" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                     partnerLinkType="tns:myProjectV3"
                     myRole="myProjectV3Provider"
                     />
        <bpel:partnerLink name="SayHelloServicePL" partnerLinkType="tns:SayHelloServicePLType" partnerRole="HelloWebServiceRole"></bpel:partnerLink>
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                  messageType="tns:myProjectV3RequestMessage"/>
                  
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output"
                  messageType="tns:myProjectV3ResponseMessage"/>
        <bpel:variable name="SayHelloServicePLResponse" messageType="ns:SayHelloResponse"></bpel:variable>
        <bpel:variable name="SayHelloServicePLRequest" messageType="ns:SayHelloRequest"></bpel:variable>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in myProjectV3.wsdl 
             -->
        <bpel:receive name="receiveInput" partnerLink="client"
                 portType="tns:myProjectV3"
                 operation="process" variable="input"
                 createInstance="yes"/>
        
        <!-- Generate reply to synchronous request -->
        <bpel:assign validate="no" name="AssignRequest">
            <bpel:copy>
                <bpel:from><bpel:literal><tns:SayHello xmlns:tns="http://www.example.org/HelloWorldService/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <in>in</in>
</tns:SayHello>
</bpel:literal></bpel:from>
                <bpel:to variable="SayHelloServicePLRequest" part="parameters"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="payload" variable="input">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:input]]></bpel:query>
                </bpel:from>
                <bpel:to part="parameters" variable="SayHelloServicePLRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[in]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="Invoke" partnerLink="SayHelloServicePL" operation="SayHello" portType="ns:HelloWorldService" inputVariable="SayHelloServicePLRequest" outputVariable="SayHelloServicePLResponse"></bpel:invoke>
        <bpel:assign validate="no" name="AssignResponse">
            <bpel:copy>
                <bpel:from><bpel:literal><tns:myProjectV3Response xmlns:tns="http://wso2.org/bps/sample" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:result>tns:result</tns:result>
</tns:myProjectV3Response>
</bpel:literal></bpel:from>
                <bpel:to variable="output" part="payload"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="parameters" variable="SayHelloServicePLResponse"></bpel:from>
                <bpel:to part="payload" variable="output">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:result]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:myProjectV3"
               operation="process" 
               variable="output"
               />
    </bpel:sequence>
</bpel:process>


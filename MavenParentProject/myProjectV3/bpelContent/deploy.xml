<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:HelloWorldService="http://www.example.org/HelloWorldService/" xmlns:sample="http://wso2.org/bps/sample">
  <process name="sample:myProjectV3">
    <active>true</active>
    <retired>false</retired>
    <in-memory>false</in-memory>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="sample:myProjectV3" port="myProjectV3Port"/>
    </provide>
    <invoke partnerLink="SayHelloServicePL">
      <service name="HelloWorldService:HelloWorldService" port="HelloWorldServiceSOAP"/>
    </invoke>
  </process>
</deploy>